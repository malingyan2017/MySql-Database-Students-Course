<h3>Filter Instructors</h3>

<h3>Table Data</h3>
<table>
<thead>
	<th>ID#</th>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Academic Rank</th>
	<th>Delete</th>
	<th>Edit</th>
</thead>
<tbody>
{{#each instructors}}
	<tr>
		<td>{{instructor_id}}</td>
		<td>{{first_name}}</td>
		<td>{{last_name}}</td>
		<td>{{name}}</td>
		<td><button onclick="deleteInstructor({{instructor_id}})">Delete</button></td>
		<td><a href="/instructor/{{instructor_id}}">Edit</button></td>
	</tr>
{{/each}}
</tbody>
</table>


