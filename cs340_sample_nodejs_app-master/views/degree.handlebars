<html>
{{#each foo}}
{{this}}
{{/each}}
<h3>Add new degree:</h3>
<form id="add_degree" action="/degree" method="post">

    Type: <input type="text" name="degree_type"><br>
    field:<input type="text" name="field"><br>
    <input type="submit" value="Submit">
 </form>
<br/>

<h3>Current degree:</h3>
<table>
    <thead> 
        <th>ID</th> 
        <th>Type</th>
        <th>field</th>
    </thead> 
    <tbody>
        {{#each degree}} 
        <tr> 
            <td>{{id}}</td> 
            <td>{{type}}</td>
            <td>{{field}}</td>
            <td><button onclick="deleteDegree({{id}})">Delete</button></td>
            <td><a href="/degree/{{id}}">Update</a></td>
        </tr> 
        {{/each}} 
    </tbody> 
</table> 
</html>